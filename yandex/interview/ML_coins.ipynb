   "source": [
    "coins = []\n",
    "\n",
    "with open('coins.in', 'r') as f:\n",
    "    n = int(f.readline())\n",
    "    for i in range(n):\n",
    "        k, m = map(int, f.readline().split())\n",
    "        score = (m + 1) / (k + 2)\n",
    "        coins.append((score, i))\n",
    "\n",
    "coins.sort()\n",
    "print('\\n'.join(str(i) for _, i in coins))"
   ]
